<div class="item">
	<img class="ui avatar image" src="<%= profile.twitter.image %>">
	<div class="content">
		<a class="header" href="/profile/<%= profile.twitter.username %>" target="_blank"><%= profile.twitter.username %></a>
		<%= profile.twitter.displayName %>
		<div class="description">
			<%= profile.links.length %>
			links.
		</div>
	</div>
</div>
